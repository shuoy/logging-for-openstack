input {
  redis {
    host => "{{ redis_host }}"
    add_field => { from_indexer => "{{ ansible_hostname }}" }
    type => "{{ file_list.type }}"
    data_type => "list"
    key => "{{ redis_key }}"
  }
}

output {
  file {
    path => "{{ debug_output }}"
  }
  elasticsearch {
    host => "{{ es_node }}"
    protocol => http
  }
}
